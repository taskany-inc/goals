{
  "name": "@taskany/issues",
  "version": "1.6.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run worker\" \"next dev\"",
    "dev:db:init": "npx prisma migrate dev --preview-feature && npx prisma db seed",
    "dev:db:run": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:db:down": "docker-compose down",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "worker": "ts-node -r dotenv/config src/utils/worker/index.ts",
    "build": "next build && npm run build:worker",
    "build:worker": "tsc -p ./src/utils/worker",
    "start": "concurrently \"npm run worker\" \"next start\"",
    "lint": "next lint && eslint --ext .js,.ts,.tsx .",
    "generate:i18n": "generate-i18n --func=tr --langs=ru,en --path='./src/{components,hooks,i18n,schema}/**/*' --getLang='./src/utils/getLang'",
    "test": "jest",
    "prepare": "husky install",
    "type-check": "npx tsc"
  },
  "prisma": {
    "seed": "ts-node -r dotenv/config --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
