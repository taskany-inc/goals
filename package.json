{
  "name": "@taskany/issues",
  "version": "2.6.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "next lint && eslint --ext .js,.ts,.tsx .",
    "type-check": "npx tsc",
    "dev": "concurrently -k \"npm run dev:worker\" \"next dev\"",
    "dev:worker": "ts-node -r dotenv/config src/utils/worker/index.ts",
    "dev:db:init": "npx prisma migrate dev && npx prisma db seed",
    "dev:db:run": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:db:down": "docker-compose down",
    "dev:db:fill": "ts-node -r dotenv/config scripts/fillDevData.ts",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma db push --force-reset",
    "build": "concurrently \"next build\" \"npm run build:worker\"",
    "build:worker": "tsc -p ./src/utils/worker",
    "build:analyze": "ANALYZE=true next build",
    "test": "jest",
    "cy:open": "ELECTRON_EXTRA_LAUNCH_ARGS=--lang=en npx cypress open",
    "test:e2e": "./scripts/e2eRun.sh",
    "test:e2e:env": "./scripts/e2eEnv.sh",
    "i18n": "generate-i18n --func=tr --langs=ru,en --path='./src/{components,hooks,i18n,schema}/**/*' --getLang='./src/utils/getLang'"
  },
  "prisma": {
    "seed": "ts-node -r dotenv/config prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.374.0",
