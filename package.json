{
  "name": "taskany-goals",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run generate:nexus\" \"npm run generate:genql\" \"npm run generate:themes\" \"next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && eslint --ext .js,.ts,.tsx .",
    "generate:themes": "nodemon --exec 'node ./scripts/themes.js' --watch 'src/design/themes/*.js'",
    "generate:nexus": "nodemon --exec 'ts-node --transpile-only -P nexus.tsconfig.json src/pages/api/graphql' --ext 'ts' --watch '*/graphql/**/*.ts' -r dotenv/config",
    "generate:genql": "nodemon --exec 'genql --schema ./graphql/schema.graphql --output ./graphql/@generated/genql'  --watch 'graphql/schema.graphql'",
    "generate": "concurrently \"npm run generate:nexus\" \"npm run generate:genql\" \"npm run generate:themes\"",
    "db:run": "docker-compose up -d",
    "db:down": "docker-compose down",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prisma": {
    "seed": "ts-node -r dotenv/config --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@geist-ui/core": "2.3.8",
    "@genql/runtime": "2.9.0",
    "@hookform/resolvers": "2.8.8",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.10.0",
    "@tippyjs/react": "4.2.6",
    "apollo-server-micro": "3.6.4",
    "apollo-tracing": "0.15.0",
    "color-contrast-checker": "2.1.0",
    "date-fns": "2.28.0",
    "graphql": "15.8.0",
    "graphql-scalars": "1.15.0",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "next": "12.1.0",
    "next-auth": "4.3.3",
    "next-intl": "2.4.1",
    "next-themes": "0.1.1",
    "nexus": "1.3.0",
    "nexus-prisma": "0.35.0",
    "nodemailer": "6.7.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.29.0",
    "react-hot-toast": "2.2.0",
